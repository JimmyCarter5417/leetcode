class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int, int> m;
        
        for (int i = 0; i < nums.size(); ++i)
        {
            int tmp = target - nums[i];
            
            if (m.find(tmp) != m.end())
                return {i, m[tmp]};
            else
                m[nums[i]] = i;
        }
        
        return {0, 0};
    }
};
